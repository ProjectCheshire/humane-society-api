{
  "name": "humane-society",
  "version": "1.0.0",
  "description": "GraphQL API to consume scraped data from the Oregon Humane Society.",
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon node_modules/.bin/babel-node server",
    "lint": "eslint . --fix --ignore-path ./.eslintignore",
    "start": "babel-node server/index.js",
    "test": "npm run lint && ./node_modules/.bin/mocha api/**/__tests__/**/*.test.js --compilers js:babel-register --require babel-polyfill"
  },
  "keywords": [
    "graphql",
    "humane society",
    "oregon",
    "api",
    "node",
    "graphiql",
    "firebase"
  ],
  "author": "Joseph Fraley",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "firebase-admin": "^4.0.4",
    "graphql": "^0.8.0",
    "graphql-server-express": "^0.4.3",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
